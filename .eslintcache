tion: \"alguna descripción\", categoryId: \"Ejemplo\" },\n      \n    { id: 6 ,titulo: \"Vegan\" ,precio: \"30\" ,\n      imagen: \"https://i.ibb.co/MnKVg7B/vegan.png\",\n      stock: 6 ,initial: 1, description: \"alguna descripción\", categoryId: \"Non-meat Meals\" },\n    \n    { id: 7, titulo: \"Veggie\", precio: \"30\", \n      imagen: \"https://i.ibb.co/Wt8TR71/veggie.png\",\n      stock: 10 ,initial: 1, description: \"alguna descripción\", categoryId: \"Non-meat Meals\" },\n      \n    { id: 8, titulo: \"Protein\" ,precio: \"20\" ,\n    imagen: \"https://i.ibb.co/PxLhX1h/beef.jpg\",\n    stock: 3 ,initial: 1, description: \"alguna descripción\", categoryId: \"Dinner & Lunch\" }\n    ]\n\n\nconst ItemListContainer = ({greeting}) => {\n\n    const [ items, setItems ] = useState([])\n\n    const {id} = useParams()\n\n    useEffect(() => {\n      // Tomo los productos que me envía App y, si hay un id, filtro todos para agarrar sólo los que\n      // tengan el id señalado\n      if(id){\n          const category = products.filter(product => product.categoryId === id )\n          setItems(category)\n          console.log(category)\n      }\n      else{\n          setItems(products)\n      }\n\n  }, [id, products]);\n\n    return (\n        <div className=\"itemList\">\n          {id ? <h1>{id}</h1>\n          :<h1>{greeting}</h1>  \n          } \n            {products.length > 0 \n            ? <ItemList products = { items } /> \n            : <h1 className=\"loading\">Loading...</h1>}\n        </div>\n    )\n}\n\nexport default ItemListContainer\n\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/ItemCount.js",["117"],"import React, { useState, useContext } from 'react'\nimport { Link } from 'react-router-dom';\nimport CartContext from './CartContext'\n\n\nconst ItemCount = ({item, addToCart}) => {\n\n    const inicial = item.initial\n    const [ contDetail , setContDetail ] = useState(inicial)\n    const name = item.titulo\n    const { isInCart } = useContext(CartContext);\n    const existeItem = isInCart(item.id);\n\n    const aumentarCont = () => {\n        if (contDetail < item.stock){\n            setContDetail(contDetail + 1)\n        }else{\n            setContDetail( item.stock )\n        }        \n    }\n    const restarCont = () => {\n        if(contDetail > item.initial){\n            setContDetail(contDetail - 1)\n        }\n    }\n   \n\n    return (\n        <>\n            { existeItem ?\n            <>\n            <h4>{\"agregaste\"+ \" \" + contDetail + \" \" + name + \" al carrito\" }</h4>\n            <Link to=\"/cart\">\n                <button className=\"addToCart\">ir al carrito</button>\n            </Link>\n            </>\n            :\n            <>\n            <div className=\"contador\">\n            <button onClick={restarCont} className=\"buttonProd\">-</button>\n            <p>{contDetail}</p>\n            <button onClick={aumentarCont} className=\"buttonProd\">+</button>\n            </div>    \n            <div>\n                <button className=\"addToCart\" \n                onClick={ () => {addToCart({ item: item, cantidad: contDetail})}}>\n                    add to cart</button>\n            </div>\n            </>}\n        </>\n    )\n}\n\nexport default ItemCount\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/ItemList.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/Item.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/ItemDetailContainer.js",["118"],"import React from 'react'\nimport GetItem from './GetItem'\n\n\nconst ItemDetailContainer = ({ }) => {\n    \n    return (\n        <div>\n            <GetItem/>\n        </div>\n    )\n}\n\nexport default ItemDetailContainer\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/Carrito.js",[],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/dropdo