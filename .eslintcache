        </BrowserRouter>\n        </>\n    )\n}\n\nexport default App\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/Main.js",["109"],"import React from 'react'\nimport { Link } from 'react-router-dom'\n\nfunction Main() {\n    return (\n        <main>\n            <div className=\"title\">\n                <h1 className=\"titleText\">MEAL PLANS</h1>\n                <h3 className=\"titleText\">2 weeks of meals planned for you!</h3>\n                <Link to=\"/itemListContainer\"><button className=\"titleText mealButton\"><a>click to check meal plans</a></button></Link>\n                <div className=\"titleImg\">\n                    <img src=\"./fondo1.jpg\" className=\"banner\" alt=\"banner\"></img>\n                    <img src=\"./fondoResponsive.jpg\" className=\"bannerRes\" alt=\"banner\"></img>\n                </div>\n            </div>\n        </main>\n    )\n}\n\nexport default Main\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/Footer.js",[],["110","111"],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/Navbar.js",["112","113"],"import React from 'react'\nimport CartWidget from './CartWidget'\nimport { Link } from 'react-router-dom'\nimport './navbar.css'\nimport DropdownNav from './DropdownNav';\n\n\nconst Navbar = ()  => {\n\n    return (\n        <>\n        <nav>\n            <ul className=\"lista\">\n                <Link to=\"/\" ><a className=\"listado\">Home</a></Link>\n                <Link to=\"/aboutMe\"><a className=\"listado\">About me</a></Link>\n                <DropdownNav/>\n                <Link to=\"/cart\"><CartWidget/></Link>\n            </ul>\n        </nav>\n        </>\n    )\n}\n\nexport default Navbar\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/Header.js",["114"],"import React from 'react'\nimport Navbar from './Navbar'\nimport { Link } from 'react-router-dom';\n\nconst Header = () => {\n    return (\n      