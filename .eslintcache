ainer\n                />\n            </Route>\n            <Route exact path=\"/item/:id\">\n                <ItemDetailContainer/>\n            </Route>\n            <Route exact path=\"/ItemListContainer\">\n                <ItemListContainer\n                    greeting=\"Products\"\n                />\n            </Route>\n            <Route exact path=\"/cart\">\n                <Carrito/>\n            </Route>\n            <Route exact path=\"/aboutMe\">\n                <AboutMe/>\n            </Route>\n        </Switch>\n        </CartState>\n        </BrowserRouter>\n        </>\n    )\n}\n\nexport default App\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/Main.js",["103","104"],"import React, { useState } from 'react'\nimport { Link } from 'react-router-dom'\n\n\nfunction Main() {\n \n    return (\n        <main>\n            <div className=\"title\">\n                <h1 className=\"titleText\">MEAL PLANS</h1>\n                <h3 className=\"titleText\">2 weeks of meals planned for you!</h3>\n                <Link to=\"/itemListContainer\"><button className=\"titleText mealButton\"><a>click to check meal plans</a></button></Link>\n                <div className=\"titleImg\">\n                    <img src=\"https://i.ibb.co/mTSRtJg/fondo1.jpg\" className=\"banner\" alt=\"banner\"></img>\n                    {/* <img src=\"../public/fondoResponsive.jpg\" className=\"bannerRes\" alt=\"banner\"></img> */}\n                </div>\n            </div>\n        </main>\n    )\n}\n\nexport default Main\n","/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/Footer.js",[],["105","106"],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/Navbar.js",["107","108","109"],"/Users/Maca/Desktop/FullStack-Proyectos/ReactCoderhouse/demo/src/components/Header.js",["110"],"import React from 'react'\nimport Navbar from './Navbar'\nimport { Link } from 'react-router-dom';\n\nconst Header = () => {\n    